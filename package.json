{
  "name": "teacherly",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku-prebuild": "rm -rf public  && mkdir public",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "start": "node app.js",
    "dev": "concurrently \"node app.js\" \"cd client && yarn start\""
  },
  "dependencies": {
    "concurrently": "^3.6.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  }
}
